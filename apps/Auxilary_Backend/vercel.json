{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*", "@repo/data/HomepageExplorerData", "@repo/eslint-config", "@repo/data","@repo/typescript-config/base.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ]
  }
  