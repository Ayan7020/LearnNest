{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*", "@repo/data/**/*", "@repo/**/*", "packages/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ]
  }
  