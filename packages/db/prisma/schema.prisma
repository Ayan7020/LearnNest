generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}



datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  Firstname String
  Lastname  String?
  email     String  @unique
  password  String?
}


model admin {
  id        Int     @id @default(autoincrement())
  Firstname String
  Lastname  String?
  email     String  @unique
  password  String? 
} 